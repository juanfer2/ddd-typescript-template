    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "express-validator": "^6.14.2",
